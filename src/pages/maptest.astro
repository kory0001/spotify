---
const artists = await fetch("https://fetch-with-astro.netlify.app/api/artists").then(response => response.json());
console.log(artists);

import Card from "../components/Card.astro";

---
<ul>
  {
  	artists.map((artist) => ( <Card title={artist.title} date={artist.date} description={artist.content} image={artist.og_image.url}/>))
  }
</ul>